{
  "author": "James Quant",
  "email": "james.quant@example.com",
  "github_username": "james_quant",
  "project_name": "DAB Pipeline",
  "project_slug": "{{ cookiecutter.project_name | slugify(separator='-') }}",
  "package_name": "{{ cookiecutter.project_name | slugify(separator='_') }}",
  "project_short_description": "Databricks Asset Bundle project for building a deployable pipeline.",
  "databricks_host": "{{ default_databricks_host() }}",
  "databricks_permission_group": "",
  "databricks_webhook_notification_id": "",
  "databricks_cluster_policy_id": "",
  "automated_sphinx_docs": ["y", "n"],
  "github_issue_template": ["y", "n"],
  "version": "0.0.0",
  "license": [
    "MIT",
    "BSD",
    "Apache Software License 2.0",
    "Proprietary"
  ],
  "copyright_year": "{% now 'utc', '%Y' %}",
  "__prompts__": {
    "author": "Author or team name",
    "email": "Author or team email address",
    "github_username": "GitHub username",
    "project_name": "Stylized name or title of your project",
    "project_slug": "Root folder of your project (usually dash separated)",
    "package_name": "Main package name (snake case)",
    "project_short_description": "Short description",
    "databricks_host": "URL of databricks workspace for deployments",
    "databricks_permission_group": "Databricks permission group for deployments",
    "databricks_webhook_notification_id": "Databricks notification id for alerts",
    "databricks_cluster_policy_id": "Databricks cluster policy id",
    "automated_sphinx_docs": "Automated build and deployment of documentation with Sphinx",
    "github_issue_template": "Predefined issue templates for GitHub"
  },
  "_extensions": ["cookiecutter.extensions.SlugifyExtension", "local_extensions.DatabricksHostExtension"],
  "_jinja2_env_vars": {"lstrip_blocks": true, "trim_blocks": true},
  "_new_lines": "\n"
}